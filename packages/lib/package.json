{
    "name": "@alleyboss/micropay-solana-x402-paywall",
    "version": "3.5.2",
    "description": "Production-ready Solana micropayments library wrapper for official x402 SDK",
    "author": "AlleyBoss",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:AlleyBo55/micropay-solana-x402-paywall.git"
    },
    "homepage": "https://solana-x402-paywall.vercel.app/",
    "keywords": [
        "solana",
        "x402",
        "paywall",
        "micropayments",
        "web3",
        "coinbase",
        "agent-payments",
        "fetch"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./express": {
            "import": {
                "types": "./dist/express/index.d.ts",
                "default": "./dist/express/index.js"
            },
            "require": {
                "types": "./dist/express/index.d.cts",
                "default": "./dist/express/index.cjs"
            }
        },
        "./next": {
            "import": {
                "types": "./dist/next/index.d.ts",
                "default": "./dist/next/index.js"
            },
            "require": {
                "types": "./dist/next/index.d.cts",
                "default": "./dist/next/index.cjs"
            }
        },
        "./agent": {
            "import": {
                "types": "./dist/agent/index.d.ts",
                "default": "./dist/agent/index.js"
            },
            "require": {
                "types": "./dist/agent/index.d.cts",
                "default": "./dist/agent/index.cjs"
            }
        },
        "./pricing": {
            "import": {
                "types": "./dist/pricing/index.d.ts",
                "default": "./dist/pricing/index.js"
            },
            "require": {
                "types": "./dist/pricing/index.d.cts",
                "default": "./dist/pricing/index.cjs"
            }
        },
        "./client": {
            "import": {
                "types": "./dist/client/index.d.ts",
                "default": "./dist/client/index.js"
            },
            "require": {
                "types": "./dist/client/index.d.cts",
                "default": "./dist/client/index.cjs"
            }
        },
        "./session": {
            "import": {
                "types": "./dist/session/index.d.ts",
                "default": "./dist/session/index.js"
            },
            "require": {
                "types": "./dist/session/index.d.cts",
                "default": "./dist/session/index.cjs"
            }
        },
        "./fetch": {
            "import": {
                "types": "./dist/fetch/index.d.ts",
                "default": "./dist/fetch/index.js"
            },
            "require": {
                "types": "./dist/fetch/index.d.cts",
                "default": "./dist/fetch/index.cjs"
            }
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run test && npm run build"
    },
    "dependencies": {
        "@x402/core": "^2.1.0",
        "@x402/next": "^2.1.0",
        "@x402/svm": "^2.1.0",
        "bs58": "^6.0.0",
        "jose": "^6.1.3"
    },
    "peerDependencies": {
        "@solana/web3.js": "^1.90.0",
        "@solana/wallet-adapter-base": "^0.9.0",
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "peerDependenciesMeta": {
        "@solana/wallet-adapter-base": {
            "optional": true
        },
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "devDependencies": {
        "@solana/wallet-adapter-base": "^0.9.27",
        "@solana/web3.js": "^1.98.4",
        "@types/bs58": "^4.0.4",
        "@types/express": "^5.0.6",
        "@types/node": "^20",
        "@types/react": "^19.2.7",
        "@types/uuid": "^10.0.0",
        "express": "^5.2.1",
        "react": "^19.2.3",
        "tsup": "^8.0.0",
        "typescript": "^5",
        "vitest": "^3.0.0"
    },
    "engines": {
        "node": ">=18"
    }
}